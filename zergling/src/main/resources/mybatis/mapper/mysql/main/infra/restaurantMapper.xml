<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.exion.mall.restaurant.RestaurantDao">
	<resultMap type="com.exion.mall.restaurant.RestaurantDto" id="resultObj"></resultMap>
	<select id="selectRest" resultMap="resultObj">
		select
			d.seq
		    ,d.foodType
		    ,d.name
		    ,d.businessHoursStart
		    ,d.businessHoursEnd
		    ,d.tel
		    ,d.webAddress
		    ,d.parkingNy
		    ,d.bookingNy
		    ,d.freeCorkageNy
		    ,d.intro
		    ,d.regDate
		    ,d.modDate
		    ,d.delNy
		from
			restaurant d
		order by d.seq desc
	</select>
	<insert id="insert">
		insert into restaurant(
			foodType
		    ,name
		    ,businessHoursStart
		    ,businessHoursEnd
		    ,tel
		    ,webAddress
		    ,parkingNy
		    ,bookingNy
		    ,freeCorkageNy
		    ,intro
		    ,regDate
		    ,modDate
		    ,delNy
		)value(
			#{foodType}
		    ,#{name}
		    ,#{businessHoursStart}
		    ,#{businessHoursEnd}
		    ,#{tel}
		    ,#{webAddress}
		    ,#{parkingNy}
		    ,#{bookingNy}
		    ,#{freeCorkageNy}
		    ,#{intro}
		    ,now()
			,now()
		    ,0
		)
	</insert>
	<select id="selectOne" resultMap="resultObj">
		select
			d.seq
		    ,d.foodType
		    ,d.name
		    ,d.businessHoursStart
		    ,d.businessHoursEnd
		    ,d.tel
		    ,d.webAddress
		    ,d.parkingNy
		    ,d.bookingNy
		    ,d.freeCorkageNy
		    ,d.intro
		    ,d.regDate
		    ,d.modDate
		    ,d.delNy
		from
			restaurant d
		where 1=1
			and d.seq = #{seq}
	</select>
	<update id="update">
		
	</update>
</mapper>